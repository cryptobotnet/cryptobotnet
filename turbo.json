{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "clean": {
      "cache": false
    },
    "format": {
      "cache": false
    },
    "lint": {
      "cache": false,
      "dependsOn": ["redis-client#build", "okx-api#build"]
    },
    "typecheck": {
      "cache": false,
      "dependsOn": ["redis-client#build", "okx-api#build"]
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["out/**", ".next/**"]
    }
  }
}
