{
  "name": "server",
  "license": "UNLICENSED",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf out .turbo",
    "format": "prettier --ignore-path ../../../.prettierignore --write . && eslint --ignore-path ../../../.eslintignore 'src/**/*.{js,ts,tsx}' --fix",
    "lint": "eslint '**/*.ts'",
    "typecheck": "tsc --pretty --noEmit",
    "start": "DEBUG=server,websocket:* ts-node-dev -r tsconfig-paths/register ./src/index.ts",
    "build": "esbuild --bundle src/index.ts --outdir=out --platform=node --target=node18"
  },
  "dependencies": {
    "align-text": "^1.0.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.5",
    "debug": "^4.3.4",
    "dedent": "^0.7.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-slow-down": "^1.5.0",
    "numeral": "^2.0.6",
    "okx-api": "*",
    "redis-client": "*"
  },
  "devDependencies": {
    "@types/align-text": "^1.0.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/dedent": "^0.7.0",
    "@types/express": "^4.17.17",
    "@types/express-slow-down": "^1.3.2",
    "@types/node": "^18.14.0",
    "esbuild": "^0.17.10",
    "eslint-config-shared": "*",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "tsconfig-shared": "*",
    "typescript": "^4.9.5"
  }
}
