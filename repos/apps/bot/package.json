{
  "name": "bot",
  "license": "UNLICENSED",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf out .turbo",
    "format": "prettier --ignore-path ../../../.prettierignore --write . && eslint --ignore-path ../../../.eslintignore 'src/**/*.{js,ts,tsx}' --fix",
    "lint": "eslint '**/*.ts'",
    "typecheck": "tsc --pretty --noEmit",
    "start": "ts-node-dev -r tsconfig-paths/register ./src/index.ts",
    "build": "tsc && tsc-alias"
  },
  "dependencies": {
    "@grammyjs/conversations": "^1.1.0",
    "@grammyjs/runner": "^1.0.4",
    "@grammyjs/transformer-throttler": "^1.2.1",
    "dedent": "^0.7.0",
    "grammy": "^1.13.1",
    "redis": "*"
  },
  "devDependencies": {
    "@types/dedent": "^0.7.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  }
}
